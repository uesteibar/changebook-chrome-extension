{
  "manifest_version": 2,

  "name": "Changebook",
  "description": "This extension notifies you when you can get the book you are visiting in amazon.",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "browser_action": {
    "default_title": "Changebook extension",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
